<?php
function form_override_form_alter(&$form, &$form_state, $form_id) {
if (!user_is_logged_in() && $form_id != 'user_login') {
$form['reset'] = array(
      '#type' => 'markup',
      '#value' => '<a class="click anno-login" href="#" >Submit</a>',
      '#weight' => 2000,
     );
 unset($form['actions']['submit']);
 unset ($form['preview']);
  unset($form['submit']);
}
}